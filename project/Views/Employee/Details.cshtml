@model project.Models.employe_Reg

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/shared/AdminMaster.cshtml";
}

<h2>Details</h2>


<div class="row mt">
<div class="col-lg-12">
    <div class="row content-panel">
        <div class="col-md-4 profile-text mt mb centered">
            <div class="right-divider hidden-sm hidden-xs">
                <h4> @Html.DisplayFor(model => model.reg_grade)</h4>
                <h6>Grade</h6>
                <h4> @Html.DisplayFor(model => model.reg_client)</h4>
                <h6>Clients</h6>
                <h4> @Html.DisplayFor(model => model.reg_depart)</h4>
                <h6>Department</h6>
            </div>
        </div>
        <!-- /col-md-4 -->
        <div class="col-md-4 profile-text">
            <h3>  @Html.DisplayFor(model => model.reg_name) </h3>
            <h6> @Html.DisplayFor(model => model.reg_role)</h6>
            <p>Contrary to popular belief, Lorem Ipsum is not simply random text. It has roots in a piece of classical Latin literature from 45 BC.</p>
            <br>
            <p></p>
        </div>
        <!-- /col-md-4 -->
        <div class="col-md-4 centered">
            <div class="profile-pic">
                <p><img src="~/EmpImages/@Url.Content(Model.reg_img)" class="img-circle"></p>
                <p>

                </p>
            </div>
        </div>
        <!-- /col-md-4 -->
    </div>
    <!-- /row -->
</div>
<!-- /col-lg-12 -->
</div>




<!-- /tab-pane -->
<div id="contact" class="tab-pane text-center">
    <div class="row content-panel">


        @*<div class="col-md-6">
                <div id="map"></div>
            </div>*@
        <!-- /col-md-6 -->
        <div class="col-md-12 detailed">
            <h4>Iformation</h4>
            <div class="col-md-8 col-md-offset-2 mt">

                <p>
                    Achievement  :   @Html.DisplayFor(model => model.reg_achieve)<br />
                </p>
                <br>
                <p>
                    Education :  @Html.DisplayFor(model => model.reg_education)<br />
                </p>
                <br>
                <p>
                    Code :  @Html.DisplayFor(model => model.code)<br />
                </p>
                <br>
               
            </div>
            
            <div class="col-md-8 col-md-offset-2 mt">
                <h4>Contacts</h4>
                <br />
                <p>
                    Phone:  @Html.DisplayFor(model => model.reg_number)<br />
                </p>
                <br>
                <p>
                    Email:  @Html.DisplayFor(model => model.reg_email)<br />
                </p>
                <br>
                <p>
                    Address :  @Html.DisplayFor(model => model.reg_address)<br />
                </p>
                <br>
                <p>

                </p>
                <br>
                <p>

                </p>
            </div>
        </div>
        <!-- /col-md-6 -->
    </div>
    <!-- /row -->
</div>
