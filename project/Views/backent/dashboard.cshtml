                <p>
                    Employees<br /> 
                </p>
                <p><i class="fa fa-comment"></i> 300 </p>
            </div>
        </div>



        <div class="col-md-4 col-sm-4 mb">
            <div class="darkblue-panel pn">
                <div class="darkblue-header">
                    <h5><a href="@Url.Action("job_view","vacancies")">Job Requests</a></h5>
                </div>
                <a href="@Url.Action("job_view","vacancies")"> <h1 class="mt"><i class="fa fa-user fa-3x"></i></h1></a>
                <p> </p>
